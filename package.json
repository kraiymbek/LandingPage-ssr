{
  "name": "stencil-ssr-starter",
  "version": "0.0.1",
  "private": true,
  "main": "server.js",
  "description": "Stencil starter with server-side rendering",
  "scripts": {
    "build": "stencil build",
    "dev": "stencil build --dev --watch --serve",
    "dev.ssr": "stencil build --dev --es5 --watch --serve",
    "ssr": "node server.js",
    "start": "npm run dev",
    "start.ssr": "npm run build && npm run ssr"
  },
  "dependencies": {
    "@stencil/core": "^0.10.0",
    "@stencil/router": "latest",
    "express": "^4.15.5"
  },
  "engines": {
    "node": ">=6.11.0"
  }
}
